<div class="container-fluid" style="background-color: #49dda5;">
  <br>
  <div class="col-xs-12 col-sm-12 col-md-10 col-md-offset-1 col-lg-10 col-lg-offset-1">
    <div class="panel">
      <div class="panel-body" style="padding-top: 0px;">
        <h2 class="text-center">Sign Up</h2>
        <%= bootstrap_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <%= devise_error_messages! %>

            <%= f.text_field :fname, class: "input-md", label: "First Name" %>

            <%= f.text_field :lname, class: "input-md", label: "Last Name" %>

            <%= f.email_field :email, class: "input-md", label: "Email" %>


            <%= f.password_field :password, autocomplete: "off", class: "input-md", label: "Password" %>
            <% if @validatable %>
             <em>(<%= @minimum_password_length %> characters minimum)</em>
            <% end %>

            <%= f.password_field :password_confirmation, autocomplete: "off", class: "input-md", label: "Password Confirmation" %>

            <%= f.select :insurance_provider, ["Aetna", "Progressive", "Humana", "UnitedHealthCare", "Cigna"] %>

            <%= f.submit "Sign Up", class: "btn btn-success" %>

        <% end %><br>

        <!--<%= render "admins/shared/links" %>-->
      </div>
    </div>
  </div>
</div>